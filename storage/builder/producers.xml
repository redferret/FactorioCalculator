<Producers>
  <Producer speed='0.5'>
    <Product name='Assembling machine 1'/>
  </Producer>
  <Producer speed='0.75'>
    <Product name='Assembling machine 2'/>
  </Producer>
  <Producer speed='1.25'>
    <Product name='Assembling machine 3'/>
  </Producer>

  <Producer speed='1.25'>
    <Product name='Chemical plant'/>
  </Producer>

  <Producer producer_type='0' speed='0.35' power='2.5'>
    <Product name='Burner mining drill'/>
  </Producer>
  <Producer producer_type='0' speed='0.5' power='3'>
    <Product name='Electric mining drill'/>
  </Producer>

  <Producer producer_type='4' speed='1'>
    <Product name='Oil refinery'/>
    <Processes>
      <Process
        name='Basic oil processing'
        image_file='Basic_oil_processing.png'
        crafting_time='5'>
        <Inputs>
          <Product name='Crude oil' amount='100'/>
        </Inputs>
        <Outputs>
          <Product name='Heavy oil' amount='30'/>
          <Product name='Light oil' amount='30'/>
          <Product name='Petroleum gas' amount='40'/>
        </Outputs>
      </Process>

      <Process
        name='Advanced oil processing'
        image_file='Advanced_oil_processing.png'
        crafting_time='5'>
        <Inputs>
          <Product name='Crude oil' amount='100'/>
          <Product name='Water' amount='50'/>
        </Inputs>
        <Outputs>
          <Product name='Heavy oil' amount='10'/>
          <Product name='Light oil' amount='45'/>
          <Product name='Petroleum gas' amount='55'/>
        </Outputs>
      </Process>

      <Process
        name='Coal liquefaction'
        image_file='Coal_liquefaction.png'
        crafting_time='5'>
        <Inputs>
          <Product name='Coal' amount='10'/>
          <Product name='Heavy oil' amount='25'/>
          <Product name='Steam' amount='50'/>
        </Inputs>
        <Outputs>
          <Product name='Heavy oil' amount='35'/>
          <Product name='Light oil' amount='15'/>
          <Product name='Petroleum gas' amount='20'/>
        </Outputs>
      </Process>
    </Processes>
  </Producer>

  <Producer speed='0.75' producer_type='4'>
    <Product name='Centrifuge'/>
    <Processes>
      <Process
        name='Uranium processing'
        image_file='Uranium_processing.png'
        crafting_time='10'>
        <Inputs>
          <Product name='Uranium ore' amount='10'/>
        </Inputs>
        <Outputs>
          <Product name='Uranium-235' amount='0.007'/>
          <Product name='Uranium-238' amount='0.993'/>
        </Outputs>
      </Process>
      <Process
        name='Nuclear fuel reprocessing'
        image_file='Nuclear_fuel_reprocessing.png'
        crafting_time='50'>
        <Inputs>
          <Product name='Used up uranium fuel cell' amount='5'/>
        </Inputs>
        <Outputs>
          <Product name='Uranium-238' amount='3'/>
        </Outputs>
      </Process>
      <Process
        name='Kovarex enrichment process'
        image_file='Kovarex_enrichment_process.png'
        crafting_time='50'>
        <Inputs>
          <Product name='Uranium-235' amount='40'/>
          <Product name='Uranium-238' amount='5'/>
        </Inputs>
        <Outputs>
          <Product name='Uranium-235' amount='41'/>
          <Product name='Uranium-238' amount='2'/>
        </Outputs>
      </Process>
    </Processes>
  </Producer>

  <Producer producer_type='3' speed='1' yield='1'>
    <Product name='Pumpjack'/>
  </Producer>
  <Producer producer_type='3' speed='1'>
    <Product name='Offshore pump'/>
  </Producer>

  <Producer producer_type='2' speed='1'>
    <Product name='Stone furnace'/>
  </Producer>
  <Producer producer_type='2' speed='2'>
    <Product name='Steel furnace'/>
  </Producer>
  <Producer producer_type='2' speed='2'>
    <Product name='Electric furnace'/>
  </Producer>
</Producers>
